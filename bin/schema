#!/usr/bin/env node

// Handle any passed arguments
require('./args');

// Otherwise get our app requirements
var schema  = require('../lib/schema.js'),
    config  = require('./config'),
    done    = require('./done');

// Generate schema documentation with our config
if(schema.generate(config)) {
  
  // TODO: Generate a proper completion message
  process.stdout.write(done(config));

}
  "version": "0.4.0",
  "keywords": [
    "JSON",
    "schema",
    "documentation",
    "tool"
  ],
  "author": {
    "name": "Matthew O'Donoghue"
  },
  "main": "./lib/matic.js",
  "bin": "./bin/matic",
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattyod/matic"
  },
  "bugs": {
    "url": "https://github.com/mattyod/matic/issues"
  },
  "dependencies": {
    "rightclick": "0.7.1",
    "col": "0.1.0",
    "underscore": "1.6.0"
  },
  "devDependencies": {
    "sinon": "1.10.0",
    "mocha": "1.19.0",
    "chai": "1.9.1",
    "sinon-chai": "2.5.0",
    "jade": "1.3.1",
    "jshint": "2.5.1"
  },
  "scripts": {
